{
    "name": "lazysoft/laravel-panel",
    "description": "A AdminPanel For Laravel",
    "license": "MIT",
    "keywords": [
        "admin-panel",
        "LazySoft",
        "lazysoft",
        "ahr1384",
        "laravel",
        "admin",
        "panel"
    ],
    "authors": [
        {
            "name": "AmirHosein Rahimi",
            "email": "amirhoseinrahimi.work@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8",
        "laravel/framework": "^9",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "LazySoft\\LaravelPanel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LazySoft\\LaravelPanel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LazySoft\\LaravelPanel\\LaravelPanelServiceProvider"
            ],
            "aliases": {
                "Panel": "LazySoft\\LaravelPanel\\Facades\\Panel"
            }
        }
    }
}